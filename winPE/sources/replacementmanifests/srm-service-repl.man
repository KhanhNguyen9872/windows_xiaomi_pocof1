        <merge script="MigXmlHelper.SourcePriority()">
          <objectSet>
            <pattern type="Registry">HKLM\SYSTEM\CurrentControlSet\Services\SrmSvc\Settings\* [*]</pattern>
            <pattern type="File">%systemdrive%\System Volume Information\SRM\* [*]</pattern>
          </objectSet>
        </merge>
      </rules>
